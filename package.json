		"postcss-simple-vars": "^7.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^5.1.11",
		"svelte-check": "^3.8.6",
		"ts-node": "^10.9.2",
		"tslib": "^2.8.1",
		"typescript": "^5.6.3",
		"vite": "^5.4.10"
	},
	"type": "module"
